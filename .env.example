# Пароли и секреты - ЗАМЕНИТЕ НА СВОИ ПЕРЕД ИСПОЛЬЗОВАНИЕМ!
POSTGRES_PASSWORD=придумайте_надежный_пароль_для_бд
NEXTAUTH_SECRET=придумайте_длинный_случайный_ключ

# URL приложения (для локальной разработки или домена)
NEXTAUTH_URL=http://localhost:3000

# Порт приложения (можно изменить если 3000 занят)
APP_PORT=3000

# Порт PostgreSQL (обычно не нужно менять)
POSTGRES_PORT=5432

# Имена контейнеров (для Dokploy, обычно не нужно менять)
COMPOSE_PROJECT_NAME=nastya

# Эти можно не менять
POSTGRES_USER=nastya_user
POSTGRES_DB=nastya_site
NODE_ENV=production
